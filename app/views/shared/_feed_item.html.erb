<li id="<%= feed_item.id %>">
  
  <span class="user">
    <%= link_to feed_item.user.name, feed_item.user %>
  </span>
  <span class="ipc">IPC:  <%= feed_item.ipc %></span>
  <span class="langpref">Language Preference:  <%= feed_item.langpref %></span>
  <span class="successes">Total Sent: <%= feed_item.successCount %>  Problems: <%= feed_item.problemCount %>  </span>
  <span class="content">Message: </span>
  <span class="content" ><%= feed_item.content %></span>
  <span class="timestamp">
    Scheduled at <%= feed_item.created_at.strftime("%m/%d/%Y %r") %>.
  </span>
  <%# if current_user?(feed_item.user) %>
    <%#= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: feed_item.content %>
  <%# end %>
</li>
